{
	"name": "overlay-node-ts",
	"module": "index.ts",
	"version": "1.0.0-rc.6",
	"type": "module",
	"license": "AGPL-3.0",
	"workspaces": [
		"./packages/cli",
		"./packages/common",
		"./packages/config",
		"./packages/contract",
		"./packages/logger",
		"./packages/node"
	],
	"scripts": {
		"start": "bun run check-ts && bun run ./packages/cli",
		"start:inspect": "bun run check-ts && bun run --inspect ./packages/cli",
		"start:unchecked": "bun run ./packages/cli",
		"start:node": "npx tsx --inspect ./packages/cli",
		"build": "bun run ./build.ts",
		"bundle": "bun run ./bundle.ts",
		"check-ts": "bunx tsc --noEmit",
		"fmt": "bunx biome check --write .",
		"check-fmt": "bunx biome check .",
		"build-schema": "bun run ./packages/contract/build.ts"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@biomejs/biome": "1.9.4",
		"tsx": "^4.19.3"
	},
	"peerDependencies": {
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"true-myth": "^8.4.0",
		"node-fetch": "^3.3.2"
	}
}
